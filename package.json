{
	"name": "sampleapp",
	"version": "0.0.1",
	"private": true,
	"license": "Fabbi.JSC",
	"scripts": {
		"android": "react-native run-android --variant=developmentDebug --appId 'com.sample.app.dev'",
		"androidDev": "react-native run-android --variant=developmentDebug --appId 'com.sample.app.dev'",
		"androidStg": "react-native run-android --variant=stagingDebug --appId 'com.sample.app.staging'",
		"androidPrd": "react-native run-android --variant=productionDebug --appId 'com.sample.app'",
		"androidReleaseDev": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --variant=devRelease --appId 'com.sample.app.dev'",
		"androidReleaseStg": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --variant=stagingRelease --appId 'com.sample.app.staging'",
		"androidReleasePrd": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --variant=productionRelease --appId 'com.sample.app'",
		"androidApkDev": "rm -rf build && rm -rf android/build && export ENVFILE=.env.dev && cd android && ./gradlew assembleDevelopmentRelease && cd ..",
		"androidApkStg": "rm -rf build && rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
		"androidApkPrd": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew assembleProductionRelease && cd ..",
		"androidAabPrd": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleProductionRelease && cd ..",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"postinstall": "patch-package && cd scripts && sh ./fix-lib.sh && cd ..",
		"pod": "cd ios && pod install && cd ..",
		"podNewArc": "cd ios && RCT_NEW_ARCH_ENABLED=1 pod install",
		"lint": "eslint ."
	},
